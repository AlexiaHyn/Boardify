{
  "id": "uno_draw_five",
  "name": "Uno Draw Five",
  "description": "Classic Uno but you must draw 5 cards when you have no other actions available.",
  "version": "1.0.0",
  "themeColor": "#dc2626",
  "backgroundEmoji": "\ud83c\udccf",
  "rules": {
    "minPlayers": 2,
    "maxPlayers": 10,
    "handSize": 7,
    "turnStructure": {
      "phases": [
        {
          "id": "play",
          "name": "Play a Card",
          "description": "Play one card that matches color, number, or symbol.",
          "isOptional": true
        },
        {
          "id": "draw",
          "name": "Draw Cards",
          "description": "If you cannot play, draw 5 cards from the draw pile.",
          "isOptional": false
        }
      ],
      "canPassTurn": false,
      "mustPlayCard": false,
      "drawCount": 5
    },
    "winCondition": {
      "type": "empty_hand",
      "description": "First player to play all their cards wins!"
    },
    "specialRules": [
      {
        "id": "uno_call",
        "name": "UNO Call",
        "trigger": "on_play",
        "description": "Must call 'UNO!' when playing your second-to-last card or draw 4 cards as penalty."
      },
      {
        "id": "draw_five_penalty",
        "name": "Draw Five Penalty",
        "trigger": "on_draw",
        "description": "When you cannot play any card, you must draw 5 cards instead of 1."
      },
      {
        "id": "wild_draw_four_challenge",
        "name": "Wild Draw Four Challenge",
        "trigger": "on_play",
        "description": "Can only play Wild Draw 4 if you have no cards matching the current color."
      }
    ]
  },
  "config": {
    "matchColor": true,
    "matchNumber": true,
    "matchType": true,
    "wildAlwaysPlayable": true,
    "stackableDraw": false,
    "reverseEqualsSkipTwoPlayers": true,
    "startWithDiscard": true,
    "excludeFromStartDiscard": [
      "wild",
      "wild_draw_four"
    ],
    "drawUntilPlayable": false,
    "colors": [
      "red",
      "yellow",
      "green",
      "blue"
    ],
    "colorEmojis": {
      "red": "\ud83d\udd34",
      "yellow": "\ud83d\udfe1",
      "green": "\ud83d\udfe2",
      "blue": "\ud83d\udd35",
      "wild": "\ud83c\udf08"
    },
    "zones": [
      {
        "id": "draw_pile",
        "name": "Draw Pile",
        "type": "deck",
        "isPublic": false
      },
      {
        "id": "discard_pile",
        "name": "Discard Pile",
        "type": "discard",
        "isPublic": true
      }
    ]
  },
