{
  "id": "crazy_eights",
  "name": "Crazy Eights",
  "description": "Match suits or ranks to empty your hand, with wild 8s that change the suit.",
  "version": "1.0.0",
  "themeColor": "#dc2626",
  "backgroundEmoji": "\ud83c\udfb4",
  "rules": {
    "minPlayers": 2,
    "maxPlayers": 8,
    "handSize": 7,
    "turnStructure": {
      "phases": [
        {
          "id": "play",
          "name": "Play a Card",
          "description": "Play a card matching the suit or rank of the top card.",
          "isOptional": true
        },
        {
          "id": "draw",
          "name": "Draw Cards",
          "description": "Draw cards until you can play one.",
          "isOptional": false
        }
      ],
      "canPassTurn": false,
      "mustPlayCard": false,
      "drawCount": 1
    },
    "winCondition": {
      "type": "empty_hand",
      "description": "First player to empty their hand wins!"
    },
    "specialRules": [
      {
        "id": "eights_wild",
        "name": "Wild Eights",
        "trigger": "on_play",
        "description": "Eights can be played on any card and allow you to choose the suit."
      },
      {
        "id": "draw_until_playable",
        "name": "Draw Until Playable",
        "trigger": "on_draw",
        "description": "When drawing, continue until you draw a playable card."
      }
    ]
  },
  "config": {
    "matchColor": true,
    "matchNumber": true,
    "matchType": false,
    "wildAlwaysPlayable": true,
    "stackableDraw": false,
    "reverseEqualsSkipTwoPlayers": false,
    "startWithDiscard": true,
    "excludeFromStartDiscard": [
      "wild"
    ],
    "drawUntilPlayable": true,
    "colors": [
      "hearts",
      "diamonds",
      "clubs",
      "spades"
    ],
    "colorEmojis": {
      "hearts": "\u2665\ufe0f",
      "diamonds": "\u2666\ufe0f",
      "clubs": "\u2663\ufe0f",
      "spades": "\u2660\ufe0f",
      "wild": "\ud83c\udf08"
    },
    "zones": [
      {
        "id": "draw_pile",
        "name": "Draw Pile",
        "type": "deck",
        "isPublic": false
      },
      {
        "id": "discard_pile",
        "name": "Discard Pile",
        "type": "discard",
        "isPublic": true
      }
    ]
  },
