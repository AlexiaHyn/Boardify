{
  "id": "go_fish",
  "name": "Go Fish",
  "description": "Classic matching card game where players collect sets of four cards of the same rank by asking opponents for cards.",
  "version": "1.0.0",
  "themeColor": "#0ea5e9",
  "backgroundEmoji": "\ud83d\udc20",
  "rules": {
    "minPlayers": 2,
    "maxPlayers": 6,
    "handSize": 7,
    "turnStructure": {
      "phases": [
        {
          "id": "ask",
          "name": "Ask for Cards",
          "description": "Ask another player for cards of a specific rank you have in your hand.",
          "isOptional": false
        }
      ],
      "canPassTurn": false,
      "mustPlayCard": false,
      "drawCount": 1
    },
    "winCondition": {
      "type": "most_points",
      "description": "Player with the most books (sets of four matching rank cards) wins when all books are collected.",
      "metadata": {
        "targetScore": 13
      }
    },
    "specialRules": [
      {
        "id": "book_collection",
        "name": "Book Collection",
        "trigger": "on_play",
        "description": "When you collect all four cards of the same rank, immediately place them face up as a book and take another turn.",
        "metadata": {}
      },
      {
        "id": "must_have_rank",
        "name": "Must Have Rank to Ask",
        "trigger": "on_play",
        "description": "You can only ask for cards of ranks you already have at least one of in your hand.",
        "metadata": {}
      },
      {
        "id": "go_fish_draw",
        "name": "Go Fish Draw",
        "trigger": "on_draw",
        "description": "When you draw the card you asked for, show it and take another turn.",
        "metadata": {}
      }
    ]
  },
  "config": {
    "matchColor": false,
    "matchNumber": true,
    "matchType": false,
    "wildAlwaysPlayable": false,
