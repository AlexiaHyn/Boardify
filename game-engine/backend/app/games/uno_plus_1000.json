{
  "id": "uno_plus_1000",
  "name": "Uno but with +1000",
  "description": "Classic Uno with an extreme +1000 Draw card that can force a player to draw 1000 cards.",
  "version": "1.0.0",
  "themeColor": "#dc2626",
  "backgroundEmoji": "\ud83d\udd25",
  "rules": {
    "minPlayers": 2,
    "maxPlayers": 10,
    "handSize": 7,
    "turnStructure": {
      "phases": [
        {
          "id": "play",
          "name": "Play a Card",
          "description": "Play one card from your hand that matches color, number, or symbol.",
          "isOptional": true
        },
        {
          "id": "draw",
          "name": "Draw a Card",
          "description": "Draw one card from the draw pile if you cannot play.",
          "isOptional": false
        }
      ],
      "canPassTurn": false,
      "mustPlayCard": false,
      "drawCount": 1
    },
    "winCondition": {
      "type": "target_score",
      "description": "First player to reach 500 points wins the game!",
      "metadata": {
        "targetScore": 500
      }
    },
    "specialRules": [
      {
        "id": "uno_call",
        "name": "UNO Call",
        "trigger": "on_play",
        "description": "Must call 'UNO!' when playing second-to-last card or draw 2 penalty cards.",
        "metadata": {}
      },
      {
        "id": "challenge_wild",
        "name": "Wild Challenge",
        "trigger": "on_play",
        "description": "Wild Draw Four and +1000 cards can be challenged if played illegally.",
        "metadata": {}
      }
    ]
  },
  "config": {
    "matchColor": true,
    "matchNumber": true,
    "matchType": true,
    "wildAlwaysPlayable": true,
    "stackableDraw": false,
    "reverseEqualsSkipTwoPlayers": true,
    "startWithDiscard": true,
    "excludeFromStartDiscard": [
      "wild_draw_four",
      "wild_draw_1000"
    ],
    "drawUntilPlayable": false,
    "colors": [
      "red",
      "yellow",
      "green",
      "blue"
    ],
    "colorEmojis": {
      "red": "\ud83d\udd34",
      "yellow": "\ud83d\udfe1",
      "green": "\ud83d\udfe2",
      "blue": "\ud83d\udd35",
      "wild": "\ud83c\udf08"
    },
    "zones": [
      {
        "id": "draw_pile",
        "name": "Draw Pile",
        "type": "deck",
        "isPublic": false
      },
      {
        "id": "discard_pile",
        "name": "Discard Pile",
        "type": "discard",
        "isPublic": true
      }
    ]
  },
