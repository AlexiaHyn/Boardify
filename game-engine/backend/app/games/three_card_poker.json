{
  "id": "three_card_poker",
  "name": "Three Card Poker",
  "description": "A simplified poker variant for exactly 3 players using poker hand rankings.",
  "version": "1.0.0",
  "themeColor": "#059669",
  "backgroundEmoji": "\u2660\ufe0f",
  "rules": {
    "minPlayers": 3,
    "maxPlayers": 3,
    "handSize": 3,
    "turnStructure": {
      "phases": [
        {
          "id": "ante",
          "name": "Place Ante",
          "description": "All players place their ante bet.",
          "isOptional": false
        },
        {
          "id": "play_fold",
          "name": "Play or Fold",
          "description": "Decide whether to play your hand or fold.",
          "isOptional": false
        }
      ],
      "canPassTurn": false,
      "mustPlayCard": false,
      "drawCount": 0
    },
    "winCondition": {
      "type": "most_points",
      "description": "Player with the highest poker hand ranking wins the round!",
      "metadata": {}
    },
    "specialRules": [
      {
        "id": "poker_rankings",
        "name": "Poker Hand Rankings",
        "trigger": "on_play",
        "description": "Hands ranked from highest to lowest: Straight Flush, Three of a Kind, Straight, Flush, Pair, High Card."
      },
      {
        "id": "ante_system",
        "name": "Ante Betting",
        "trigger": "on_turn_end",
        "description": "All players must ante before seeing their cards. Winner takes all antes."
      }
    ]
  },
  "config": {
    "matchColor": false,
    "matchNumber": false,
    "matchType": false,
    "wildAlwaysPlayable": false,
    "stackableDraw": false,
    "reverseEqualsSkipTwoPlayers": false,
    "startWithDiscard": false,
    "excludeFromStartDiscard": [],
    "drawUntilPlayable": false,
    "zones": [
      {
        "id": "draw_pile",
        "name": "Deck",
        "type": "deck",
        "isPublic": false
      },
      {
        "id": "player_hands",
