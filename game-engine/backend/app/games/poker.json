          "isOptional": false
        }
      ],
      "canPassTurn": true,
      "mustPlayCard": false,
      "drawCount": 0
    },
    "winCondition": {
      "type": "most_points",
      "description": "Player with the best five-card poker hand wins the pot!",
      "metadata": {}
    },
    "specialRules": [
      {
        "id": "blinds",
        "name": "Blind Bets",
        "trigger": "on_turn_start",
        "description": "Small blind and big blind post forced bets before cards are dealt.",
        "metadata": {}
      },
      {
        "id": "community_cards",
        "name": "Community Cards",
        "trigger": "on_play",
        "description": "Five community cards are dealt: three on the flop, one on the turn, one on the river.",
        "metadata": {}
      },
      {
        "id": "showdown",
        "name": "Showdown",
        "trigger": "on_turn_end",
        "description": "Players reveal hands and best five-card combination wins.",
        "metadata": {}
      }
    ]
  },
