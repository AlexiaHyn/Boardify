{
  "id": "poker",
  "name": "Poker",
  "description": "Classic card game where players bet and make the best five-card hand using community cards and hole cards.",
  "version": "1.0.0",
  "themeColor": "#0F172A",
  "backgroundEmoji": "\u2660\ufe0f",
