{
  "id": "texas_holdem",
  "name": "Texas Hold'em Poker",
  "description": "The most popular poker variant where players make the best 5-card hand using 2 hole cards and 5 community cards.",
  "version": "1.0.0",
  "themeColor": "#059669",
  "backgroundEmoji": "\u2660\ufe0f",
  "rules": {
    "minPlayers": 2,
    "maxPlayers": 10,
    "handSize": 2,
    "turnStructure": {
      "phases": [
        {
          "id": "preflop_betting",
          "name": "Pre-Flop Betting",
          "description": "First betting round after receiving hole cards.",
          "isOptional": false
        },
        {
          "id": "flop_betting",
          "name": "Flop Betting",
          "description": "Betting round after the flop (first 3 community cards).",
          "isOptional": false
        },
        {
          "id": "turn_betting",
          "name": "Turn Betting",
          "description": "Betting round after the turn (4th community card).",
          "isOptional": false
        },
        {
          "id": "river_betting",
          "name": "River Betting",
          "description": "Final betting round after the river (5th community card).",
          "isOptional": false
        }
      ],
      "canPassTurn": true,
      "mustPlayCard": false,
      "drawCount": 0
    },
    "winCondition": {
      "type": "most_points",
      "description": "Player with the best 5-card hand wins the pot!",
      "metadata": {}
    },
    "specialRules": [
      {
        "id": "blinds",
        "name": "Forced Bets",
        "trigger": "on_turn_start",
        "description": "Small and big blinds must be posted before cards are dealt."
      },
      {
        "id": "community_cards",
        "name": "Community Cards",
        "trigger": "on_play",
        "description": "Five community cards are dealt face-up that all players can use."
      },
      {
        "id": "showdown",
        "name": "Showdown",
        "trigger": "on_turn_end",
        "description": "Best 5-card hand using any combination of hole and community cards wins."
      },
      {
        "id": "all_in",
        "name": "All-In Protection",
        "trigger": "on_play",
        "description": "Players with insufficient chips can go all-in and remain eligible for side pots."
      }
    ]
  },
  "config": {
    "matchColor": false,
    "matchNumber": false,
    "matchType": false,
    "wildAlwaysPlayable": false,
    "stackableDraw": false,
    "reverseEqualsSkipTwoPlayers": false,
    "startWithDiscard": false,
    "excludeFromStartDiscard": [],
    "drawUntilPlayable": false,
    "colors": [
