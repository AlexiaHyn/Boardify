{
  "id": "uno_draw_three",
  "name": "UNO Draw 3",
  "description": "Classic UNO but you must draw 3 cards when you can't play anything.",
  "version": "1.0.0",
  "themeColor": "#dc2626",
  "backgroundEmoji": "\ud83c\udfaf",
  "rules": {
    "minPlayers": 2,
    "maxPlayers": 10,
    "handSize": 7,
    "turnStructure": {
      "phases": [
        {
          "id": "play",
          "name": "Play a Card",
          "description": "Play one card that matches color or number, or play a Wild card.",
          "isOptional": true
        },
        {
          "id": "draw",
          "name": "Draw Cards",
          "description": "Draw 3 cards if you cannot play any card.",
          "isOptional": false
        }
      ],
      "canPassTurn": false,
      "mustPlayCard": true,
      "drawCount": 3
    },
    "winCondition": {
      "type": "empty_hand",
      "description": "First player to play all their cards wins!"
    },
    "specialRules": [
      {
        "id": "uno_call",
        "name": "UNO Call",
        "trigger": "on_play",
        "description": "Must say UNO when you have one card left or draw 4 penalty cards."
      },
      {
        "id": "draw_three",
        "name": "Draw Three Rule",
        "trigger": "on_turn_end",
        "description": "When you cannot play any card, draw 3 cards instead of 1."
      },
      {
        "id": "wild_draw_four_challenge",
        "name": "Wild Draw 4 Challenge",
        "trigger": "on_play",
        "description": "Players can challenge Wild Draw 4 if they think you had a matching color."
      }
    ]
  },
  "config": {
    "matchColor": true,
    "matchNumber": true,
    "matchType": false,
    "wildAlwaysPlayable": true,
    "stackableDraw": false,
    "reverseEqualsSkipTwoPlayers": true,
    "startWithDiscard": true,
    "excludeFromStartDiscard": [
      "wild_draw_four"
    ],
    "drawUntilPlayable": false,
    "colors": [
      "red",
      "yellow",
      "green",
      "blue"
    ],
    "colorEmojis": {
      "red": "\ud83d\udd34",
      "yellow": "\ud83d\udfe1",
      "green": "\ud83d\udfe2",
      "blue": "\ud83d\udd35",
      "wild": "\ud83c\udf08"
    },
    "zones": [
      {
        "id": "draw_pile",
        "name": "Draw Pile",
        "type": "deck",
        "isPublic": false
      },
      {
        "id": "discard_pile",
        "name": "Discard Pile",
        "type": "discard",
        "isPublic": true
      }
    ]
  },
